{
  "type": "module",
  "version": "0.1.1",
  "name": "a-element-inline-style",
  "private": true,
  "description": "设置行内样式",
  "scripts": {
    "b": "rollup --config rollup.config.js && tsc -p tsconfig.types.json",
    "clean:package": "node scripts/clean-package-json.js",
    "build": "jja rm dist && pnpm run b && pnpm run clean:package",
    "diff": "jja pkg --diff",
    "eslint": "jja cls && eslint src",
    "beautify": "pnpm run prettier",
    "prettier": "jja cls && prettier . --write",
    "push:version": "gvv",
    "push": "gvv",
    "vjj": "vjj",
    "test": "jja cls && rollup --config rollup.config.test.js && node test/out/test/index.mjs",
    "prepublishOnly": "pjj"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@qqi/rollup-external": "^1.0.9",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.1.0",
    "a-node-tools": "^4.2.19",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "gvv": "^0.1.5",
    "jja": "^2.3.14",
    "pjj": "^1.0.3",
    "prettier": "^3.6.2",
    "rollup": "^4.46.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vjj": "^1.0.9"
  },
  "dependencies": {
    "a-type-of-js": "^1.0.7"
  }
}
